class Employee
{
	int eno;
	String ename;
	float salary;
	void get(int x,String y,float z)
	{
		eno=x;
		ename=y;
		salary=z;
	}
	void show()
	{
		System.out.println(eno+" "+ename+" "+salary);
	}
}
class Details
{
	public static void main(String[] args)
	{
		Employee e1=new Employee();
		e1.get(123,"xxx",1500);
		Employee e2=new Employee();
		e2.get(124,"yyy",1400);
		e1.show();
		e2.show();
	}
}
output
123 xxx 1500.0
124 yyy 1400.0
